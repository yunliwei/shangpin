<%= render 'adminmenu'%>
<div class="pages">
  <div data-page="projects" class="page no-toolbar no-navbar">
    <div class="page-content">

      <div class="navbarpages">
        <div class="navbar_home_link"><a href="index.html"><img src="images/icons/white/home.png" alt="" title="" /></a></div>
        <div class="navbar_page_center">现在报名</div>
        <div class="menu_open_icon_white"><a href="#" class="open-panel"><img src="images/menu_open.png" alt="" title="" /></a></div>
      </div>
<script charset="utf-8" src="/editor/kindeditor.js"></script>
<script charset="utf-8" src="/editor/lang/zh-CN.js"></script>

<div id="main">
  <div class="breadcrumb-wrapper">
    <div class="container">
      <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12 row ">
        <h3 class="title">报名信息</h3>
        <br>
        <br>
        <table class="table table-striped table-bordered">
          <thead>
          <tr>

            <th>参加报名的活动</th>
            <th>标题</th>
            <th>内容</th>



            <th>创建时间</th>
            <th>操作</th>
          </tr>
          </thead>
          <tbody>
          <%@contacts.each do |f| %>
              <tr>
                <td><%= f.name %></td>
                <td><%= link_to f.title,contact_path(f)%></td>
                <td><%= f.tel %></td>
                <td><%= f.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
                <td><%= link_to '删除',f, method: :delete, data: { confirm: 'Are you sure?' },class:"icon-trash" %></td>
              </tr>
          <% end %>
          </tbody>
        </table>

      </div>
    </div>
  </div>
</div>
    </div>
  </div>
</div>